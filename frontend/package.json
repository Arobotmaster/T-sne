{
  "name": "mof-tsne-visualization",
  "version": "1.0.0",
  "description": "MOF数据t-SNE交互式可视化前端应用",
  "main": "index.html",
  "scripts": {
    "dev": "python -m http.server 3000",
    "build": "mkdir -p dist && cp -r static/* dist/",
    "test": "jest",
    "lint": "eslint src/",
    "format": "prettier --write src/"
  },
  "keywords": [
    "mof",
    "tsne",
    "visualization",
    "data-science"
  ],
  "author": "MOF Visualization Team",
  "license": "MIT",
  "dependencies": {
    "bootstrap": "^5.3.2",
    "plotly.js": "^2.27.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.4",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.1.2",
    "jsdom": "^23.0.1",
    "prettier": "^3.1.0"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/src/tests/setup.js"
    ],
    "testMatch": [
      "**/tests/**/*.test.js",
      "**/__tests__/**/*.js"
    ]
  }
}
